<div class="container">
    <h1 class="mt-5">All Articles</h1>
    <% if (articles.length > 0) { %>
        <% articles.forEach((article) => { %>
            <%- include('../partials/user_article', { article: article })%>
        <% }) %>
    <% } else { %>
        <p>No articles found.</p>
    <% } %>
</div>
